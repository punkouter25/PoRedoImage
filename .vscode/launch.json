{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch and Debug",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/ImageGc/Server/bin/Debug/net9.0/ImageGc.Server.dll",
            "args": [],
            "cwd": "${workspaceFolder}/ImageGc/Server",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        {
            "name": "Attach to Process",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
